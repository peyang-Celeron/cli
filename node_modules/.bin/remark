#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/runner/work/cli/cli/node_modules/.pnpm/remark-cli@9.0.0/node_modules/remark-cli/cli.js/node_modules:/home/runner/work/cli/cli/node_modules/.pnpm/remark-cli@9.0.0/node_modules/remark-cli/node_modules:/home/runner/work/cli/cli/node_modules/.pnpm/remark-cli@9.0.0/node_modules:/home/runner/work/cli/cli/node_modules/.pnpm/node_modules:/home/runner/work/cli/cli/node_modules:/home/runner/work/cli/node_modules:/home/runner/work/node_modules:/home/runner/node_modules:/home/node_modules:/node_modules:/home/runner/work/cli/cli/node_modules/remark-cli/cli.js/node_modules:/home/runner/work/cli/cli/node_modules/remark-cli/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../remark-cli/cli.js" "$@"
else 
  exec node  "$basedir/../remark-cli/cli.js" "$@"
fi

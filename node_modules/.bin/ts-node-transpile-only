#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/runner/work/cli/cli/node_modules/.pnpm/ts-node@9.1.1_typescript@4.1.3/node_modules/ts-node/dist/bin-transpile.js/node_modules:/home/runner/work/cli/cli/node_modules/.pnpm/ts-node@9.1.1_typescript@4.1.3/node_modules/ts-node/dist/node_modules:/home/runner/work/cli/cli/node_modules/.pnpm/ts-node@9.1.1_typescript@4.1.3/node_modules/ts-node/node_modules:/home/runner/work/cli/cli/node_modules/.pnpm/ts-node@9.1.1_typescript@4.1.3/node_modules:/home/runner/work/cli/cli/node_modules/.pnpm/node_modules:/home/runner/work/cli/cli/node_modules:/home/runner/work/cli/node_modules:/home/runner/work/node_modules:/home/runner/node_modules:/home/node_modules:/node_modules:/home/runner/work/cli/cli/node_modules/ts-node/dist/bin-transpile.js/node_modules:/home/runner/work/cli/cli/node_modules/ts-node/dist/node_modules:/home/runner/work/cli/cli/node_modules/ts-node/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../ts-node/dist/bin-transpile.js" "$@"
else 
  exec node  "$basedir/../ts-node/dist/bin-transpile.js" "$@"
fi

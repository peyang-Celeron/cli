#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/runner/work/cli/cli/node_modules/.pnpm/msgpack@1.0.3/node_modules/msgpack/bin/json2msgpack/node_modules:/home/runner/work/cli/cli/node_modules/.pnpm/msgpack@1.0.3/node_modules/msgpack/bin/node_modules:/home/runner/work/cli/cli/node_modules/.pnpm/msgpack@1.0.3/node_modules/msgpack/node_modules:/home/runner/work/cli/cli/node_modules/.pnpm/msgpack@1.0.3/node_modules:/home/runner/work/cli/cli/node_modules/.pnpm/node_modules:/home/runner/work/cli/cli/node_modules:/home/runner/work/cli/node_modules:/home/runner/work/node_modules:/home/runner/node_modules:/home/node_modules:/node_modules:/home/runner/work/cli/cli/node_modules/msgpack/bin/json2msgpack/node_modules:/home/runner/work/cli/cli/node_modules/msgpack/bin/node_modules:/home/runner/work/cli/cli/node_modules/msgpack/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../msgpack/bin/json2msgpack" "$@"
else 
  exec node  "$basedir/../msgpack/bin/json2msgpack" "$@"
fi

#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/runner/work/cli/cli/node_modules/.pnpm/mustache@4.1.0/node_modules/mustache/bin/mustache/node_modules:/home/runner/work/cli/cli/node_modules/.pnpm/mustache@4.1.0/node_modules/mustache/bin/node_modules:/home/runner/work/cli/cli/node_modules/.pnpm/mustache@4.1.0/node_modules/mustache/node_modules:/home/runner/work/cli/cli/node_modules/.pnpm/mustache@4.1.0/node_modules:/home/runner/work/cli/cli/node_modules/.pnpm/node_modules:/home/runner/work/cli/cli/node_modules:/home/runner/work/cli/node_modules:/home/runner/work/node_modules:/home/runner/node_modules:/home/node_modules:/node_modules:/home/runner/work/cli/cli/node_modules/.pnpm/i18n@0.13.2/node_modules/i18n/node_modules:/home/runner/work/cli/cli/node_modules/.pnpm/i18n@0.13.2/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../mustache@4.1.0/node_modules/mustache/bin/mustache" "$@"
else 
  exec node  "$basedir/../../../../../mustache@4.1.0/node_modules/mustache/bin/mustache" "$@"
fi

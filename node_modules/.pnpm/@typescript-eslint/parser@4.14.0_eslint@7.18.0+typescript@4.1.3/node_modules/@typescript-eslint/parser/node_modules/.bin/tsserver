#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/runner/work/cli/cli/node_modules/.pnpm/typescript@4.1.3/node_modules/typescript/bin/tsserver/node_modules:/home/runner/work/cli/cli/node_modules/.pnpm/typescript@4.1.3/node_modules/typescript/bin/node_modules:/home/runner/work/cli/cli/node_modules/.pnpm/typescript@4.1.3/node_modules/typescript/node_modules:/home/runner/work/cli/cli/node_modules/.pnpm/typescript@4.1.3/node_modules:/home/runner/work/cli/cli/node_modules/.pnpm/node_modules:/home/runner/work/cli/cli/node_modules:/home/runner/work/cli/node_modules:/home/runner/work/node_modules:/home/runner/node_modules:/home/node_modules:/node_modules:/home/runner/work/cli/cli/node_modules/.pnpm/@typescript-eslint/parser@4.14.0_eslint@7.18.0+typescript@4.1.3/node_modules/@typescript-eslint/parser/node_modules:/home/runner/work/cli/cli/node_modules/.pnpm/@typescript-eslint/parser@4.14.0_eslint@7.18.0+typescript@4.1.3/node_modules/@typescript-eslint/node_modules:/home/runner/work/cli/cli/node_modules/.pnpm/@typescript-eslint/parser@4.14.0_eslint@7.18.0+typescript@4.1.3/node_modules:/home/runner/work/cli/cli/node_modules/.pnpm/@typescript-eslint/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../../typescript@4.1.3/node_modules/typescript/bin/tsserver" "$@"
else 
  exec node  "$basedir/../../../../../../../typescript@4.1.3/node_modules/typescript/bin/tsserver" "$@"
fi

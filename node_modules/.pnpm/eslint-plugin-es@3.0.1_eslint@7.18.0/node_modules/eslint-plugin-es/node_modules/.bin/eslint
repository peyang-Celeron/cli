#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/runner/work/cli/cli/node_modules/.pnpm/eslint@7.18.0/node_modules/eslint/bin/eslint.js/node_modules:/home/runner/work/cli/cli/node_modules/.pnpm/eslint@7.18.0/node_modules/eslint/bin/node_modules:/home/runner/work/cli/cli/node_modules/.pnpm/eslint@7.18.0/node_modules/eslint/node_modules:/home/runner/work/cli/cli/node_modules/.pnpm/eslint@7.18.0/node_modules:/home/runner/work/cli/cli/node_modules/.pnpm/node_modules:/home/runner/work/cli/cli/node_modules:/home/runner/work/cli/node_modules:/home/runner/work/node_modules:/home/runner/node_modules:/home/node_modules:/node_modules:/home/runner/work/cli/cli/node_modules/.pnpm/eslint-plugin-es@3.0.1_eslint@7.18.0/node_modules/eslint-plugin-es/node_modules:/home/runner/work/cli/cli/node_modules/.pnpm/eslint-plugin-es@3.0.1_eslint@7.18.0/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../eslint@7.18.0/node_modules/eslint/bin/eslint.js" "$@"
else 
  exec node  "$basedir/../../../../../eslint@7.18.0/node_modules/eslint/bin/eslint.js" "$@"
fi

#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/runner/work/cli/cli/node_modules/.pnpm/make-plural@4.3.0/node_modules/make-plural/bin/make-plural/node_modules:/home/runner/work/cli/cli/node_modules/.pnpm/make-plural@4.3.0/node_modules/make-plural/bin/node_modules:/home/runner/work/cli/cli/node_modules/.pnpm/make-plural@4.3.0/node_modules/make-plural/node_modules:/home/runner/work/cli/cli/node_modules/.pnpm/make-plural@4.3.0/node_modules:/home/runner/work/cli/cli/node_modules/.pnpm/node_modules:/home/runner/work/cli/cli/node_modules:/home/runner/work/cli/node_modules:/home/runner/work/node_modules:/home/runner/node_modules:/home/node_modules:/node_modules:/home/runner/work/cli/cli/node_modules/.pnpm/messageformat@2.3.0/node_modules/messageformat/node_modules:/home/runner/work/cli/cli/node_modules/.pnpm/messageformat@2.3.0/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../make-plural@4.3.0/node_modules/make-plural/bin/make-plural" "$@"
else 
  exec node  "$basedir/../../../../../make-plural@4.3.0/node_modules/make-plural/bin/make-plural" "$@"
fi

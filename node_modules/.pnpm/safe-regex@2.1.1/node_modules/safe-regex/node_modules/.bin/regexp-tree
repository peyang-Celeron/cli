#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/runner/work/cli/cli/node_modules/.pnpm/regexp-tree@0.1.21/node_modules/regexp-tree/bin/regexp-tree/node_modules:/home/runner/work/cli/cli/node_modules/.pnpm/regexp-tree@0.1.21/node_modules/regexp-tree/bin/node_modules:/home/runner/work/cli/cli/node_modules/.pnpm/regexp-tree@0.1.21/node_modules/regexp-tree/node_modules:/home/runner/work/cli/cli/node_modules/.pnpm/regexp-tree@0.1.21/node_modules:/home/runner/work/cli/cli/node_modules/.pnpm/node_modules:/home/runner/work/cli/cli/node_modules:/home/runner/work/cli/node_modules:/home/runner/work/node_modules:/home/runner/node_modules:/home/node_modules:/node_modules:/home/runner/work/cli/cli/node_modules/.pnpm/safe-regex@2.1.1/node_modules/safe-regex/node_modules:/home/runner/work/cli/cli/node_modules/.pnpm/safe-regex@2.1.1/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../regexp-tree@0.1.21/node_modules/regexp-tree/bin/regexp-tree" "$@"
else 
  exec node  "$basedir/../../../../../regexp-tree@0.1.21/node_modules/regexp-tree/bin/regexp-tree" "$@"
fi
